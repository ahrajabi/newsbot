{
  "manifest_version": 2,

  "name": "Khabare Man",
  "description": "Khabare Man Notification!",
  "version": "0.00009",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "Click here!"
  },
  "content_scripts": [
    {
      "matches" : ["http://*/*"],
      "js": ["bg.js"]
    }
  ],
  "permissions": [
   "activeTab",
    "stoarge",
    "tabs"
   ]
}
